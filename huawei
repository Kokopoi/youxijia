<script>
var shunxu=0//声明变量记录位置数
$(".btn_box input").eq(0).addClass("current")//让第1个按是当前项效果
$(".banner_box img").hide().eq(0).show()//除了第1张图其他都隐藏
$(".btn_box input").click(
	function(){
		shunxu = $(this).index()//得到当前按钮位置数赋值给变量shunxu
		$(".banner_box img").hide().eq(shunxu).show()//图片都隐藏，只有和按钮位置一样的显示
		$(".btn_box input").removeClass("current")//都不加当前项的效果
		$(this).addClass("current")//只有点的这个按钮添加当前项的效果
	}
)

</script>





