<script>
function listmore(){

$(".newslist").animate({top:-40},1000, function(){
	$(".newslist").css("top",0)
	$(".newslist dd:last").after($(".newslist dd:first"))
	})}
	
	
	






var y = setInterval(listmore,2000)
$(".newbox").mouseenter(
	function(){
		clearInterval(y)
		}
).mouseleave(
	function(){
		y =setInterval(listmore,2000)
		}
)



</script>




